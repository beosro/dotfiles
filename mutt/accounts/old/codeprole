# Account Settings ------------------------------

set from      = "code.prole@gmail.com"
set sendmail  = "/usr/bin/msmtp -a codeprole"

# Default inbox.
set spoolfile = "+codeprole/INBOX"

# Other special folders.
set mbox      = "+codeprole/archive"
set postponed = "+codeprole/drafts"
set record    = "+codeprole/sent"

color status green default
set my_acct   = "code prole"

unmailboxes *
mailboxes =codeprole/INBOX =codeprole/archive =codeprole/drafts =codeprole/sent
